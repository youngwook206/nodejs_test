CREATE TABLE `NPM_DMNEX` (
	`cid` VARCHAR(100) NOT NULL COMMENT 'PK' COLLATE 'utf8_bin',
	`DMNEX_ID` VARCHAR(8) NOT NULL COMMENT '수요조사 ID' COLLATE 'utf8_general_ci',
	`PRPS_TECH_NM` VARCHAR(200) NOT NULL COMMENT '제안 기술 명' COLLATE 'utf8_general_ci',
	`PLNN_ANCM_TP_SE` VARCHAR(6) NOT NULL COMMENT '기획 공고 유형 구분[TQ2]' COLLATE 'utf8_general_ci',
	`PRPS_IDST_TECL_CD` VARCHAR(7) NULL DEFAULT NULL COMMENT '제안 산업 기술분류 코드' COLLATE 'utf8_general_ci',
	`ADJ_IDST_TECL_CD` VARCHAR(7) NULL DEFAULT NULL COMMENT '조정 산업 기술분류 코드' COLLATE 'utf8_general_ci',
	`ADJ_RSN` VARCHAR(2000) NULL DEFAULT NULL COMMENT '조정 사유' COLLATE 'utf8_general_ci',
	`TECH_DVLM_GOLE_CN` MEDIUMTEXT NULL DEFAULT NULL COMMENT '기술 개발 목표 내용' COLLATE 'utf8_general_ci',
	`TECH_DVLM_CN` MEDIUMTEXT NULL DEFAULT NULL COMMENT '기술 개발 내용' COLLATE 'utf8_general_ci',
	`DVLM_NCSR_CN` MEDIUMTEXT NULL DEFAULT NULL COMMENT '개발 필요 내용' COLLATE 'utf8_general_ci',
	`DMST_DVLM_TRND_CN` MEDIUMTEXT NULL DEFAULT NULL COMMENT '국내 개발 동향 내용' COLLATE 'utf8_general_ci',
	`EXTR_DVLM_TRND_CN` MEDIUMTEXT NULL DEFAULT NULL COMMENT '국외 개발 동향 내용' COLLATE 'utf8_general_ci',
	`EXPE_EFCT_CN` MEDIUMTEXT NULL DEFAULT NULL COMMENT '기대 효과 내용' COLLATE 'utf8_general_ci',
	`PRPS_ORGN_ID` VARCHAR(8) NULL DEFAULT NULL COMMENT '제안 기관 ID' COLLATE 'utf8_general_ci',
	`ORGN_CL_SE` VARCHAR(6) NULL DEFAULT NULL COMMENT '기관 분류 구분[SG5]' COLLATE 'utf8_general_ci',
	`LOC_ZNE_SE` VARCHAR(6) NULL DEFAULT NULL COMMENT '소재지 지역 구분[TS2]' COLLATE 'utf8_general_ci',
	`PRPS_MBR_ID` VARCHAR(8) NOT NULL COMMENT '제안 인력 ID' COLLATE 'utf8_general_ci',
	`DMNEX_RCVE_STT_SE` VARCHAR(6) NULL DEFAULT NULL COMMENT '수요조사 접수 상태 구분[NG0]' COLLATE 'utf8_general_ci',
	`LAST_RCVE_DE` DATETIME NOT NULL COMMENT '최종 접수 일자',
	`SBJT_PLNN_ANCM_ID` VARCHAR(6) NULL DEFAULT NULL COMMENT '과제 기획 공고 ID' COLLATE 'utf8_general_ci',
	`ATCH_DOC_ID` VARCHAR(8) NULL DEFAULT NULL COMMENT '첨부 문서 ID' COLLATE 'utf8_general_ci',
	`FRST_REG_MBRS_ID` VARCHAR(8) NOT NULL COMMENT '최초 등록 회원 ID' COLLATE 'utf8_general_ci',
	`FRST_REG_DT` DATETIME NOT NULL COMMENT '최종 등록 일시',
	`LAST_MODF_MBRS_ID` VARCHAR(8) NOT NULL COMMENT '최종 수정 회원 ID' COLLATE 'utf8_general_ci',
	`LAST_MODF_DT` DATETIME NOT NULL COMMENT '최종 수정 일시',
	`DTRS_RFLT_SE` VARCHAR(6) NOT NULL COMMENT '데이터 반영 구분[NJ8]' COLLATE 'utf8_general_ci',
	`CNLK_STT_SE` VARCHAR(6) NOT NULL COMMENT '연계상태구분[NK7]' COLLATE 'utf8_general_ci',
	`CNLK_TRS_DT` DATETIME NULL DEFAULT current_timestamp() COMMENT '연계송신일시',
	`CNLK_RCV_DT` DATETIME NULL DEFAULT NULL COMMENT '연계수신일시',
	`CNLK_ERR_CN` VARCHAR(4000) NULL DEFAULT NULL COMMENT '연계오류내용' COLLATE 'utf8_general_ci',
	PRIMARY KEY (`cid`, `DMNEX_ID`) USING BTREE
)
COMMENT='품목 및 기술 수요조사'
COLLATE='utf8_general_ci'
ENGINE=InnoDB
;
