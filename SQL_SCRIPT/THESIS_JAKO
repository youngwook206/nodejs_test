CREATE TABLE `THESIS_JAKO` (
	`CID` VARCHAR(50) NOT NULL COLLATE 'utf8_general_ci',
	`datestamp` VARCHAR(14) NOT NULL COLLATE 'utf8_general_ci',
	`setspec` VARCHAR(7) NOT NULL COLLATE 'utf8_general_ci',
	`title_h` VARCHAR(400) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`title_e` VARCHAR(400) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`title_eq` VARCHAR(400) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`author` VARCHAR(2000) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`affiliation` VARCHAR(2000) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`publish_date` VARCHAR(50) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`publisher` VARCHAR(400) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`journal_title` VARCHAR(400) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`issn` VARCHAR(400) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`volume` VARCHAR(50) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`page` VARCHAR(50) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`subject` VARCHAR(2000) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`abstract_h` TEXT(65535) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`abstract_e` TEXT(65535) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`toc` VARCHAR(2000) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`keyword` VARCHAR(2000) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`location_org` VARCHAR(2000) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`publication_country` VARCHAR(50) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`language` VARCHAR(50) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`reference` VARCHAR(400) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`data_flag` VARCHAR(50) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`format` VARCHAR(50) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`deep_link` VARCHAR(1000) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`contents_flag` VARCHAR(50) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`charge` VARCHAR(50) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`contents_url` VARCHAR(1000) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`cnlk_stt_se` VARCHAR(6) NULL DEFAULT 'NK700S' COLLATE 'utf8_general_ci',
	`cnlk_trs_dt` DATETIME NULL DEFAULT current_timestamp(),
	`cnlk_rcv_dt` DATETIME NULL DEFAULT NULL,
	`cnlk_err_cn` VARCHAR(4000) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`koi` VARCHAR(100) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	PRIMARY KEY (`CID`) USING BTREE
)
COLLATE='utf8_general_ci'
ENGINE=InnoDB
;
