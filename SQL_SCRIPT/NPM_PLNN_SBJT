CREATE TABLE `NPM_PLNN_SBJT` (
	`cid` VARCHAR(100) NOT NULL COMMENT 'PK' COLLATE 'utf8_bin',
	`PLNN_SBJT_ID` VARCHAR(8) NOT NULL COMMENT '기획 과제 ID' COLLATE 'utf8_general_ci',
	`PLNN_YY` VARCHAR(4) NULL DEFAULT NULL COMMENT '기획 년도' COLLATE 'utf8_general_ci',
	`PLNN_TECH_FILD_CD` VARCHAR(7) NULL DEFAULT NULL COMMENT '기획 기술 분야 코드' COLLATE 'utf8_general_ci',
	`RFP_ADMN_NO` VARCHAR(200) NULL DEFAULT NULL COMMENT 'RFP 관리 번호' COLLATE 'utf8_general_ci',
	`PLNN_SBJT_NM` VARCHAR(300) NOT NULL COMMENT '기획 과제 명' COLLATE 'utf8_general_ci',
	`DVLM_GOLE_CN` MEDIUMTEXT NULL DEFAULT NULL COMMENT '개발 목표 내용' COLLATE 'utf8_general_ci',
	`GOV_SPRT_NCSR_CN` MEDIUMTEXT NULL DEFAULT NULL COMMENT '정부 지원 필요 내용' COLLATE 'utf8_general_ci',
	`PUSE_FILD_CN` MEDIUMTEXT NULL DEFAULT NULL COMMENT '활용 분야 내용' COLLATE 'utf8_general_ci',
	`SPRT_PRID_CN` MEDIUMTEXT NULL DEFAULT NULL COMMENT '지원 기간 내용' COLLATE 'utf8_general_ci',
	`SPRT_BUD_CN` MEDIUMTEXT NULL DEFAULT NULL COMMENT '지원 예산 내용' COLLATE 'utf8_general_ci',
	`PLNN_SBJT_SE` VARCHAR(6) NOT NULL COMMENT '기획 과제 구분[TQ3]' COLLATE 'utf8_general_ci',
	`PLNN_WAY_SE` VARCHAR(6) NOT NULL COMMENT '기획 방식 구분[NI6]' COLLATE 'utf8_general_ci',
	`PLNN_CYLE_SE` VARCHAR(6) NOT NULL COMMENT '기획 주기 구분[NI7]' COLLATE 'utf8_general_ci',
	`PLNN_SBJT_SELE_DE` DATETIME NULL DEFAULT NULL COMMENT '기획 과제 선정 일자',
	`PLNN_SBJT_CONF_DE` DATETIME NULL DEFAULT NULL COMMENT '기획 과제 확정 일자',
	`SBJT_DVLM_TP_SE` VARCHAR(6) NULL DEFAULT NULL COMMENT '과제 개발 유형 구분[DU9]' COLLATE 'utf8_general_ci',
	`PBOFR_TP_SE` VARCHAR(6) NULL DEFAULT NULL COMMENT '공모 유형 구분[SAO]' COLLATE 'utf8_general_ci',
	`CORTIUM_CPST_SE` VARCHAR(6) NULL DEFAULT NULL COMMENT '컨소시엄 구성 구분[DK5]' COLLATE 'utf8_general_ci',
	`NEPSA_SUM_GD` DECIMAL(5,2) NOT NULL COMMENT 'NEPSA 합계 점수',
	`NEPSA_PRIO_RK` INT(5) NOT NULL COMMENT 'NEPSA 우선 순위',
	`PRIO_RK_SCR_SE` VARCHAR(6) NULL DEFAULT NULL COMMENT '우선 순위 등급 구분[NC9]' COLLATE 'utf8_general_ci',
	`PRIO_SPRT_RSLT_SE` VARCHAR(6) NULL DEFAULT NULL COMMENT '우선 지원 결과 구분[NI4]' COLLATE 'utf8_general_ci',
	`PLNN_VRFY_RSLT_SE` VARCHAR(6) NULL DEFAULT NULL COMMENT '기획 검증 결과 구분[NI5]' COLLATE 'utf8_general_ci',
	`TTL_GVCTM_AMT` BIGINT(20) NOT NULL COMMENT '총 정부출연금 금액',
	`FRYY_GVCTM_AMT` BIGINT(20) NOT NULL COMMENT '1차년도 정부출연금 금약',
	`TTL_DVLM_MONTH_CNT` DECIMAL(5,2) NOT NULL COMMENT '총 개발 개월수',
	`FRYY_DVLM_MONTH_CNT` DECIMAL(5,2) NOT NULL COMMENT '1차년도 개발 개월수',
	`SVORGN_TP_LST` VARCHAR(2000) NULL DEFAULT NULL COMMENT '주관 기관 유형 목록' COLLATE 'utf8_general_ci',
	`TFEE_IMPS_SE` VARCHAR(6) NULL DEFAULT NULL COMMENT '기술료 징수 구분[GB6]' COLLATE 'utf8_general_ci',
	`RMK` MEDIUMTEXT NULL DEFAULT NULL COMMENT '비고' COLLATE 'utf8_general_ci',
	`VLID_YN` VARCHAR(1) NOT NULL COMMENT '유효 여부' COLLATE 'utf8_general_ci',
	`PLNN_BSNS_CD` VARCHAR(6) NULL DEFAULT NULL COMMENT '기획 사업 코드' COLLATE 'utf8_general_ci',
	`SPRT_BUD_AMT` BIGINT(20) NOT NULL COMMENT '지원 예산 금액',
	`FRST_REG_MBRS_ID` VARCHAR(8) NOT NULL COMMENT '최초 등록 회원 ID' COLLATE 'utf8_general_ci',
	`FRST_REG_DT` DATETIME NOT NULL COMMENT '최종 등록 일시',
	`LAST_MODF_MBRS_ID` VARCHAR(8) NOT NULL COMMENT '최종 수정 회원 ID' COLLATE 'utf8_general_ci',
	`LAST_MODF_DT` DATETIME NOT NULL COMMENT '최종 수정 일시',
	`ANCM_SCHDLD_YY` VARCHAR(4) NULL DEFAULT NULL COMMENT '공고 예정 년도' COLLATE 'utf8_general_ci',
	`APRV_GRP_ID` VARCHAR(8) NULL DEFAULT NULL COMMENT '결재 그룹 ID' COLLATE 'utf8_general_ci',
	`RGLT_IPRV_REVW_MTTR` VARCHAR(4000) NULL DEFAULT NULL COMMENT '규정 개선 검토 사항' COLLATE 'utf8_general_ci',
	`PLNN_IPMT_WAY_SE` VARCHAR(6) NOT NULL COMMENT '기획 실시 방식 구분[NS9]' COLLATE 'utf8_general_ci',
	`DTRS_RFLT_SE` VARCHAR(6) NOT NULL COMMENT '데이터 반영 구분[NJ8]' COLLATE 'utf8_general_ci',
	`CNLK_STT_SE` VARCHAR(6) NOT NULL COMMENT '연계상태구분[NK7]' COLLATE 'utf8_general_ci',
	`CNLK_TRS_DT` DATETIME NULL DEFAULT current_timestamp() COMMENT '연계송신일시',
	`CNLK_RCV_DT` DATETIME NULL DEFAULT NULL COMMENT '연계수신일시',
	`CNLK_ERR_CN` VARCHAR(4000) NULL DEFAULT NULL COMMENT '연계오류내용' COLLATE 'utf8_general_ci',
	PRIMARY KEY (`cid`, `PLNN_SBJT_ID`) USING BTREE
)
COMMENT='기획 과제'
COLLATE='utf8_general_ci'
ENGINE=InnoDB
;
